{
  "name": "fast-extract",
  "version": "1.2.2",
  "description": "Extract contents from various archive types (tar, tar.bz2, tar.gz, tar.xz, tgz, zip)",
  "keywords": [
    "fast",
    "extract",
    "extract-tar",
    "extract-bz2",
    "extract-gz",
    "extract-xz",
    "extract-tgz",
    "extract-zip",
    "decompress",
    "decompress-tar",
    "decompress-bz2",
    "decompress-gz",
    "decompress-xz",
    "decompress-tgz",
    "decompress-zip",
    ".tar",
    "tar",
    ".tar.bz2",
    "tar.bz2",
    ".tar.gz",
    "tar.gz",
    ".tar.xz",
    "tar.xz",
    ".tgz",
    "tgz",
    ".zip",
    "zip"
  ],
  "homepage": "https://github.com/kmalakoff/fast-extract",
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/fast-extract.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "deploy": "tsds deploy",
    "format": "biome check --write --unsafe src/ test/",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "nvu engines npm test",
    "version": "tsds version"
  },
  "dependencies": {
    "bl": "^3.0.0",
    "buffer-v6-polyfill": "^1.0.5",
    "call-once-fn": "^1.0.1",
    "content-disposition": "^0.5.4",
    "core-js": "^3.39.0",
    "end-of-stream": "^1.4.4",
    "flush-write-stream": "^2.0.0",
    "lodash.throttle": "^4.1.1",
    "mkpath": "^1.0.0",
    "mock-require-lazy": "^1.0.1",
    "next-tick": "^1.1.0",
    "progress-stream": "^2.0.0",
    "pumpify": "^2.0.1",
    "queue-cb": "^1.4.1",
    "readable-stream": "^2.3.7",
    "require_optional": "^1.0.1",
    "rimraf2": "^2.8.2",
    "signal-exit": "3.0.7",
    "tar-iterator": "^1.1.3",
    "temp-suffix": "^0.1.1",
    "unbzip2-stream": "^1.4.3",
    "zip-iterator": "^1.1.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.2",
    "cr": "^0.1.0",
    "depcheck": "^1.4.7",
    "fs-iterator": "^5.1.1",
    "fs-stats-spys": "^1.1.0",
    "lzma-native": "^8.0.6",
    "ts-dev-stack": "^1.2.1"
  },
  "peerDependencies": {
    "lzma-native": "^6.0.1"
  },
  "peerDependenciesMeta": {
    "lzma-native": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.mjs",
    "targets": [
      "cjs",
      "esm"
    ]
  }
}
